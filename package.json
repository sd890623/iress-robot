{
  "name": "iress-robot",
  "version": "0.0.1",
  "description": "Submission for a toy robot code challenge",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./src",
    "build": "webpack",
    "start": "node lib/bundle.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sd890623/iress-robot.git"
  },
  "keywords": [
    "javascript",
    "es6",
    "webpack",
    "node",
    "robot"
  ],
  "author": "Di Sun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sd890623/iress-robot/issues"
  },
  "homepage": "https://github.com/sd890623/iress-robot#readme",
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.2",
    "jest": "20.0.4",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {}
}
